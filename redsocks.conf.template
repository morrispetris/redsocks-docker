base {
    log_debug = off;
    log_info = on;
    log = "stderr";
    daemon = off;           # IMPORTANT: stays in foreground for Docker
    redirector = iptables;  
}

redsocks {
    local_ip = 0.0.0.0;
    local_port = 12345;
    ip = {{PROXY_SERVER}};
    port = {{PROXY_PORT}};
    type = {{PROXY_TYPE}};
}
